{
  "name": "donchenkoin/test-package",
  "description": "Example Laravel package",
  "type": "package",
  "license": "MIT",
  "homepage": "https://github.com/DonchenkoIN/test-repository.git",
  "autoload": {
    "psr-4": {
      "DonchenkoIn\\TestPackage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DonchenkoIn\\TestPackage\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "DonchenkoIN",
      "email": "ihordon4encko@yandex.ru"
    }
  ],
  "require-dev": {
    "orchestra/testbench": "^10.1"
  },
  "require": {
    "php": "^8.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Illuminate\\Support\\ServiceProvider\\TestRepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "https://github.com/DonchenkoIN/test-repository.git",
      "options": {
        "symlink": false
      }
    }
  ]
}
